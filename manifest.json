{
  "manifest_version": 2,

  "name": "Steam Community Cookie Parser",
  "description": "Get Steam Community Cookir for Idle-master",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "steam.html"
  },
  "permissions": [
    "*://*.steamcommunity.com/",
    "cookies",
    "clipboardWrite"
  ]
}
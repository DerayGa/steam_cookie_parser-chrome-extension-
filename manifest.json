{
  "manifest_version": 2,

  "name": "Steam Community Cookie Parser",
  "description": "Get Steam Community Cookir for Idle-master",
  "version": "1.0",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/19.png",
      "38": "icon/38.png"
    },
    "default_popup": "steam.html"
  },
  "permissions": [
    "*://*.steamcommunity.com/",
    "cookies",
    "clipboardWrite"
  ]
}